      <column type="guint"/>
      <!-- column-name source -->
      <column type="gchararray"/>
      <!-- column-name destination -->
      <column type="gchararray"/>
      <!-- column-name taille -->
      <column type="guint"/>
      <!-- column-name temps -->
      <column type="gchararray"/>
      <!-- column-name decodage -->
      <column type="gchararray"/>
